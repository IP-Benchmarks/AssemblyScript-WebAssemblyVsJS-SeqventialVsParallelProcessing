{
    "targets": {
        "debug": {
            "binaryFile": "wasm/unoptimized.wasm",
            "textFile": "wasm/unoptimized.wat",
            "tsdFile": "wasm/types.d.ts",
            "debug": true,
            "exportRuntime": true
        },
        "release": {
            "binaryFile": "wasm/optimized.wasm",
            "textFile": "wasm/optimized.wat",
            "tsdFile": "wasm/types.d.ts",
            "optimizeLevel": 3,
            "shrinkLevel": 0,
            "converge": false,
            "noAssert": true,
            "exportRuntime": true
        },

        "quicksort": {
