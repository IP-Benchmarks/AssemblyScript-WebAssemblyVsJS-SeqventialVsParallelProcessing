            "shrinkLevel": 0,
            "converge": false,
            "noAssert": true,
            "exportRuntime": true
        },

        "array-generator": {
            "binaryFile": "wasm/array-generator/optimized.wasm",
            "textFile": "wasm/array-generator/optimized.wat",
            "tsdFile": "wasm/array-generator/types.d.ts",
            "optimizeLevel": 3,
            "shrinkLevel": 0,
            "converge": false,
            "noAssert": true,
