        "format:check": "nx format:check",
        "update": "nx migrate latest",
        "workspace-generator": "nx workspace-generator",
        "dep-graph": "nx dep-graph",
        "help": "nx help",
        " ": "",
        "build-standalone:node": "ng build node ",
        "watch-standalone:node": "ng build node --watch",
        "serve-standalone:node": "ng serve node",
        "  ": "",
        "build:react": "ng build react ",
        "watch:react": "ng build react --watch",
        "serve:react": "ng serve react",
        "   ": "",
        "build:api": "ng build api ",
        "watch:api": "ng build api --watch",
        "serve:api": "ng serve api",
        "    ": "",
        "start": "npm run build:asc && npm-run-all --parallel serve:*",
        "      ": "",
        "build:asc": "npm-run-all --parallel asbuild:* && npm-run-all --parallel copy:*"
